<h1> <%= @place["name"] %> </h1>

<ul>
  <% for entry in @entry %>
    <h2>
        <%= entry["title"] %> 
    </h2>
        <%= entry["posted_on"] %> 
    <p>
        <%= entry["description"] %> 
    </p>
  <% end %>
</ul>

<p>
  <a href="/entries/new?place_id=<%= @place.id %>">New entry for <%= @place.name %></a>
</p>  

<p>
  <a href="/places">Back to All Places</a>
</p>